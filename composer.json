{
    "name": "koala/lib",
    "description": "Scan URLs FROM A CSV FILE AND REPORT INACCESSIBLE URLs",
    "keywords": ["laravel", "repository", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "jonathan",
            "email": "flykoala@aliyun.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "dingo/api": "2.0.0-alpha1",
        "laravel/framework": "5.6.*",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Koala\\Lib\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Koala\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
